{
  "name": "leaflet-contextmenu",
  "version": "1.5.2",
  "description": "A context menu for Leaflet",
  "main": "dist/leaflet.contextmenu.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "concat": "cat src/copyright.js src/Map.ContextMenu.js src/Mixin.ContextMenu.js src/end.js > dist/leaflet.contextmenu.js",
    "compress": "node_modules/uglify-js/bin/uglifyjs dist/leaflet.contextmenu.js -m --comments -o dist/leaflet.contextmenu.min.js",
    "build": "npm run concat && npm run compress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gunyakov/Leaflet.contextmenu.git"
  },
  "keywords": [
    "leaflet",
    "contextmenu",
    "mixin",
    "context",
    "menu"
  ],
  "author": "Adam Ratcliffe, Oleg Gunyakov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gunyakov/Leaflet.contextmenu/issues"
  },
  "homepage": "https://github.com/gunyakov/Leaflet.contextmenu",
  "devDependencies": {
    "uglifycss": "^0.0.25",
    "uglify-js": "^3.15.1"
  }
}
